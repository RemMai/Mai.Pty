        <ImplicitUsings>enable</ImplicitUsings>
        <Nullable>enable</Nullable>
    </PropertyGroup>

    <ItemGroup>
      <PackageReference Include="sch.pty.net" Version="0.3.36-pre" />
    </ItemGroup>

    <ItemGroup>
      <None Remove="os64\conpty.dll" />
